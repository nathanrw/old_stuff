class matrix():
    def __init__(self,data):
        self.data = data
        self.index = 0
    def __iter__(self):
        return self
    def next(self):
        if self.index == len(data)-1:
            raise StopIteration
        self.index = self.index+1
        return self.data[self.index]

for char in matrix('lololol'):
    print char
